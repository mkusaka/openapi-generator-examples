  "license": "MIT",
  "scripts": {
    "build": "yarn build:current && yarn build:clearify",
    "current": "yarn --cwd current",
    "clearify": "yarn --cwd clearify",
    "build:current": "yarn current && yarn current build",
    "build:clearify": "yarn clearify && yarn clearify build",
    "bundlesize": "yarn build && bundlesize --config bundlesize.json"
  },
  "devDependencies": {
    "bundlesize": "^0.18.1"
  },
  "description": "My webpack project"
}
